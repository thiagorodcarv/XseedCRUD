<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lojacrud">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ItemHistoricoPackage.ItemHistoricoActivity"></activity>
        <activity
            android:name=".ImagemDetalhesPopUp"
            android:theme="@style/AppTheme.CustomTheme" />
        <activity android:name=".DetalhesActivity" />
        <activity android:name=".CarrinhoPackage.Carrinho"
            android:theme="@style/AppTheme.CustomTheme.Carrinho"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".PopUp"
            android:theme="@style/AppTheme.CustomTheme" />
        <activity android:name=".CadastroActivity" />
        <activity android:name=".MainActivity"
            android:configChanges="orientation">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>